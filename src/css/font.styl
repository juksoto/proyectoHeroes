@font-face {
    font-family: 'icomoon';
    src:  url('fonts/icomoon.eot?ldzokc');
    src:  url('fonts/icomoon.eot?ldzokc#iefix') format('embedded-opentype'),
      url('fonts/icomoon.ttf?ldzokc') format('truetype'),
      url('fonts/icomoon.woff?ldzokc') format('woff'),
      url('fonts/icomoon.svg?ldzokc#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
    font-display: block;
  }
  
  [class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icomoon' !important;
    speak: never;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
  
    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  
  .icon-search:before {
    content: "\f002";
  }
  .icon-facebook:before {
    content: "\f082";
  }
  .icon-thumbs-o-up:before {
    content: "\f087";
  }
  .icon-thumbs-o-down:before {
    content: "\f088";
  }
  .icon-twitter1:before {
    content: "\f099";
  }
  